# Container user settings
USER_NAME=your_username
GROUP_NAME=your_group
UID=1000
GID=1000
PASSWORD=your_secure_password

# Workspace and robot settings
WORKSPACE_DIR=/home/your_username/share
NETWORK_IF=your_network_interface
ROBOT_NAME=your_robot_name

# HSR package repository credentials
# Obtain from Toyota HSR support or HSR documentation
HSR_USER=your_hsr_username
HSR_PASSWORD=your_hsr_password
